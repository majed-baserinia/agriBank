{
	"name": "@htsc/ui",
	"version": "0.0.1",
	"type": "module",
	"description": "set of components to build react apps with",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FoHoOV/htsc",
		"directory": "packages/ui"
	},
	"exports": {
		"./components/*": {
			"import": "./dist/components/*/index.js",
			"types": "./dist/components/*/index.d.ts"
		},
		"./stores/*": {
			"import": "./dist/stores/*/index.js",
			"types": "./dist/stores/*/index.d.ts"
		},
		"./tailwind-config": {
			"import": "./dist/tailwindConfig.js",
			"default": "./dist/tailwindConfig.js"
		},
		"./utils": {
			"import": "./dist/utils.js",
			"types": "./dist/utils.d.ts"
		}
	},
	"scripts": {
		"build": "tsc && vite build",
		"dev": "vite",
		"format": "prettier --write .",
		"lint": "eslint .",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@emotion/cache": "^11.11.0",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@glidejs/glide": "^3.6.2",
		"@htsc/eslint-plugin": "0.0.1",
		"@htsc/i18n": "0.0.1",
		"@htsc/post-message": "0.0.1",
		"@htsc/prettier-config": "0.0.1",
		"@mui/icons-material": "^5.14.1",
		"@mui/lab": "^6.0.0-beta.14",
		"@mui/material": "^5.14.3",
		"@mui/styled-engine-sc": "^5.12.0",
		"@mui/utils": "^6.1.6",
		"@types/glidejs__glide": "^3.6.5",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.1",
		"@types/stylis": "^4.2.0",
		"@vitejs/plugin-react": "^4.3.3",
		"eslint": "^9.12.0",
		"i18next": "^23.2.5",
		"prettier": "^3.3.3",
		"react-camera-pro": "^1.4.0",
		"react-date-object": "^2.1.9",
		"react-hook-form": "^7.45.0",
		"react-i18next": "^13.0.1",
		"react-modal-sheet": "^2.2.0",
		"react-multi-date-picker": "^4.4.1",
		"stylis": "^4.3.0",
		"stylis-plugin-rtl": "^2.1.1",
		"typescript": "~5.6.2",
		"vite": "^5.4.9",
		"vite-plugin-dts": "^4.3.0",
		"vite-plugin-lib-inject-css": "^2.1.1",
		"vite-tsconfig-paths": "^5.0.1",
		"zustand": "^4.3.8"
	},
	"peerDependencies": {
		"@emotion/cache": "^11.11.0",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@glidejs/glide": "^3.6.2",
		"@htsc/i18n": "0.0.1",
		"@htsc/post-message": "0.0.1",
		"@mui/icons-material": "^5.14.1",
		"@mui/lab": "^6.0.0-beta.14",
		"@mui/material": "^5.14.3",
		"@mui/styled-engine-sc": "^5.12.0",
		"i18next": "^23.2.5",
		"react": "^18.3.1",
		"react-camera-pro": "^1.4.0",
		"react-date-object": "^2.1.9",
