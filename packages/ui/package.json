{
	"name": "@agribank/ui",
	"version": "3.1.2",
	"type": "module",
	"description": "set of components to build react apps with",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FoHoOV/agribank",
		"directory": "packages/ui"
	},
	"contributors": [
		"@FoHoOV",
		"@SaeedMohammadiha"
	],
	"keywords": [
		"agribank",
		"htsc"
	],
	"files": [
		"dist/",
		"package.json",
		"README.md",
		"CHANGELOG.md"
	],
	"exports": {
		"./components/*": {
			"import": "./dist/components/*/index.js",
			"types": "./dist/lib/components/*/index.d.ts"
		},
		"./expanded-theme": {
			"import": "./dist/expanded-theme.js",
			"types": "./dist/lib/components/expanded-theme.d.ts"
		},
		"./stores/*": {
			"import": "./dist/stores/*/index.js",
			"types": "./dist/lib/stores/*/index.d.ts"
		},
		"./tailwind-config": {
			"import": "./dist/tailwindConfig.js",
			"default": "./dist/tailwindConfig.js",
			"require": "./dist/tailwindConfig.js",
			"types": "./dist/tailwind.base.config.d.ts"
		},
		"./utils": {
			"import": "./dist/utils.js",
			"types": "./dist/lib/utils/index.d.ts"
		},
		"./assets/themes/base.json": {
			"import": "./dist/themes/base.json"
		}
	},
	"scripts": {
		"build": "tsc --build && vite build",
		"dev": "vite",
		"format": "prettier --write .",
		"lint": "eslint .",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@agribank/eslint-plugin": "^0.3.0",
		"@agribank/i18n": "^0.3.0",
		"@agribank/post-message": "^0.2.0",
		"@agribank/prettier-config": "^0.0.6",
		"@emotion/cache": "^11.11.0",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@glidejs/glide": "^3.6.2",
		"@mui/icons-material": "^6.1.8",
		"@mui/material": "^6.1.8",
		"@mui/styled-engine-sc": "^6.1.8",
		"@mui/utils": "^6.1.8",
		"@types/glidejs__glide": "^3.6.5",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.1",
		"@types/stylis": "^4.2.0",
		"@vitejs/plugin-react": "^4.3.3",
		"eslint": "^9.12.0",
		"i18next": "^23.2.5",
		"prettier": "^3.3.3",
		"react-camera-pro": "^1.4.0",
		"react-date-object": "^2.1.9",
		"react-hook-form": "^7.45.0",
		"react-i18next": "^13.0.1",
		"react-modal-sheet": "^2.2.0",
		"react-multi-date-picker": "^4.4.1",
		"stylis": "^4.3.0",
		"stylis-plugin-rtl": "^2.1.1",
		"typescript": "~5.6.2",
		"vite": "^5.4.9",
		"vite-plugin-dts": "^4.3.0",
		"vite-plugin-lib-inject-css": "^2.1.1",
		"vite-tsconfig-paths": "^5.0.1",
		"zustand": "^5.0.3"
	},
	"peerDependencies": {
		"@agribank/i18n": "^0.3.0",
		"@emotion/cache": "^11.11.0",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@glidejs/glide": "^3.6.2",
		"@mui/icons-material": "^6.1.8",
		"@mui/material": "^6.1.8",
		"@mui/styled-engine-sc": "^6.1.8",
		"@mui/utils": "^6.1.8",
		"i18next": "^23.2.5",
		"react": "^18.3.1",
		"react-camera-pro": "^1.4.0",
		"react-date-object": "^2.1.9",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.45.0",
		"react-i18next": "^13.0.1",
		"react-modal-sheet": "^2.2.0",
		"react-multi-date-picker": "^4.4.1",
		"stylis": "^4.3.0",
		"stylis-plugin-rtl": "^2.1.1",
		"tailwindcss": "^3.3.3",
		"zustand": "^5.0.3"
	},
	"sideEffects": [
		"**/*.css"
	],
	"publishConfig": {
		"access": "public"
	}
}
