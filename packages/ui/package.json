{
	"name": "@agribank/ui",
	"version": "6.6.0",
	"type": "module",
	"description": "set of components to build react apps with",
	"author": "majed-baserinia",
	"keywords": [
		"agribank",
		"htsc"
	],
	"files": [
		"dist/",
		"package.json",
		"README.md",
		"CHANGELOG.md"
	],
	"exports": {
		"./components/*": {
			"import": "./dist/components/*/index.js",
			"types": "./dist/lib/components/*/index.d.ts"
		},
		"./expanded-theme": {
			"import": "./dist/expanded-theme.js",
			"types": "./dist/lib/components/expanded-theme.d.ts"
		},
		"./stores/*": {
			"import": "./dist/stores/*/index.js",
			"types": "./dist/lib/stores/*/index.d.ts"
		},
		"./tailwind-config": {
			"import": "./dist/tailwindConfig.js",
			"types": "./dist/tailwind.base.config.d.ts",
			"require": "./dist/tailwindConfig.js",
			"default": "./dist/tailwindConfig.js"
		},
		"./utils": {
			"import": "./dist/utils.js",
			"types": "./dist/lib/utils/index.d.ts"
		},
		"./assets/themes/base.json": {
			"import": "./dist/themes/base.json"
		}
	},
	"scripts": {
		"build": "tsc --build && vite build",
		"dev": "vite",
		"format": "prettier --write .",
		"lint": "eslint .",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@agribank/eslint-plugin": "0.4.0",
		"@agribank/i18n": "0.6.2",
		"@agribank/post-message": "0.4.2",
		"@agribank/prettier-config": "0.0.7",
		"@emotion/cache": "11.11.0",
		"@emotion/react": "11.11.1",
		"@emotion/styled": "11.11.0",
		"@glidejs/glide": "3.6.2",
		"@mui/icons-material": "6.4.3",
		"@mui/material": "6.4.3",
		"@mui/styled-engine-sc": "6.4.3",
		"@mui/utils": "6.4.3",
		"@tanstack/router-devtools": "1.102.1",
		"@tanstack/react-router": "1.102.1",
		"@types/glidejs__glide": "3.6.5",
		"@types/react": "19.0.0",
		"@types/react-dom": "19.0.0",
		"@types/stylis": "4.2.0",
		"@vitejs/plugin-react": "4.3.4",
		"eslint": "9.12.0",
		"i18next": "24.2.2",
		"prettier": "3.3.3",
		"react-camera-pro": "1.4.0",
		"react-date-object": "2.1.9",
		"react-hook-form": "7.45.0",
		"react-i18next": "15.4.0",
		"react-modal-sheet": "2.2.0",
		"react-multi-date-picker": "4.4.1",
		"stylis": "4.3.6",
		"stylis-plugin-rtl": "2.1.1",
		"typescript": "~5.6.3",
		"vite": "6.1.0",
		"vite-plugin-dts": "4.5.0",
		"vite-plugin-lib-inject-css": "2.2.1",
		"vite-tsconfig-paths": "5.1.4",
		"zustand": "5.0.3",
		"swiper": "11.1.11"
	},
	"peerDependencies": {
		"@agribank/i18n": "0.6.2",
		"@emotion/cache": "11.11.0",
		"@emotion/react": "11.11.1",
		"@emotion/styled": "11.11.0",
		"@glidejs/glide": "3.6.2",
		"@mui/icons-material": "6.4.3",
		"@mui/material": "6.4.3",
		"@mui/styled-engine-sc": "6.4.3",
		"@mui/utils": "6.4.3",
		"@tanstack/react-router": "1.102.1",
		"@tanstack/router-devtools": "1.102.1",
		"i18next": "24.2.2",
		"react": "19.0.0",
		"react-camera-pro": "1.4.0",
		"react-date-object": "2.1.9",
		"react-dom": "19.0.0",
		"react-hook-form": "7.45.0",
		"react-i18next": "15.4.0",
		"react-modal-sheet": "2.2.0",
		"react-multi-date-picker": "4.4.1",
		"stylis": "4.3.6",
		"stylis-plugin-rtl": "2.1.1",
		"tailwindcss": "3.3.3",
		"zustand": "5.0.3"
	},
	"sideEffects": [
		"**/*.css"
	],
	"publishConfig": {
		"access": "public"
	}
}
