{
	"name": "@agribank/baran-playground",
	"version": "0.7.1",
	"type": "module",
	"description": "an environment to mimic baran app for testing microfronts created for baran",
	"author": "@FoHoOV",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FoHoOV/agribank",
		"directory": "packages/baran-playground"
	},
	"contributors": [
		"@FoHoOV",
		"@SaeedMohammadiha"
	],
	"keywords": [
		"agribank",
		"htsc"
	],
	"files": [
		"dist/",
		"README.md",
		"package.json",
		"CHANGELOG.md"
	],
	"exports": {
		".": {
			"import": "./dist/cli.js",
			"types": "./dist/cli.d.ts"
		}
	},
	"bin": {
		"baran-playground": "./dist/cli.js"
	},
	"scripts": {
		"dev": "vite --port 9000",
		"build": "tsc --build && vite build --mode cli && vite build --mode ui",
		"lint": "eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"preview": "vite preview --port 9000",
		"generate-clients": "pnpm generate-clients:customer-info",
		"generate-clients:customer-info": "pnpm agribank generate-clients --uri http://10.0.189.141:32102/swagger/v1/swagger.json --out src/services/.generated/customer-management --replace-endpoint-regex /api/,/customer-management/ --replace-endpoint-regex /customer-management\\/CustomerManager/,/CustomerManager/ --skip-spec-validations true"
	},
	"dependencies": {
		"@agribank/baran-typed-querykit": "^6.0.1",
		"@agribank/i18n": "^0.4.0",
