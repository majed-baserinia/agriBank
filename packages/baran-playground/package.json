		"@emotion/styled": "^11.11.0",
		"@hookform/resolvers": "^3.9.1",
		"@mui/icons-material": "^6.1.8",
		"@mui/material": "^6.1.8",
		"@mui/styled-engine-sc": "^6.1.8",
		"@tanstack/react-query": "^5.62.3",
		"@tanstack/react-router": "^1.95.0",
		"@tanstack/zod-adapter": "^1.95.0",
		"axios": "^1.7.7",
		"axios-retry": "^4.5.0",
		"commander": "^12.1.0",
		"i18next": "^23.2.5",
		"immer": "^10.1.1",
		"notistack": "^3.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-draggable": "^4.4.6",
		"react-hook-form": "^7.45.0",
		"react-i18next": "^13.0.1",
		"stylis": "^4.3.0",
		"stylis-plugin-rtl": "^2.1.1",
		"vite-plugin-dts": "^4.3.0",
		"zod": "^3.23.8",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@agribank/cli": "^0.2.2",
		"@agribank/eslint-plugin": "^0.3.0",
		"@agribank/prettier-config": "^0.0.6",
		"@agribank/vite-plugin": "^9.0.0",
		"@eslint/js": "^9.13.0",
		"@openapitools/openapi-generator-cli": "^2.15.0",
		"@playwright/test": "^1.49.0",
		"@tanstack/react-query-devtools": "^5.62.3",
		"@tanstack/router-devtools": "^1.95.0",
		"@tanstack/router-plugin": "^1.95.0",
		"@types/glidejs__glide": "^3.6.5",
		"@types/node": "^22.8.2",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.1",
		"@types/stylis": "^4.2.0",
		"@vitejs/plugin-react": "^4.3.3",
		"autoprefixer": "^10.4.14",
		"dotenv": "^16.4.7",
