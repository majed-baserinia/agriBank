{
	"name": "@agribank/cli",
	"version": "0.0.6",
	"type": "module",
	"description": "command line tools for creating and updating agribank apps",
	"author": "@FoHoOV",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FoHoOV/agribank",
		"directory": "packages/cli"
	},
	"contributors": [
		"@FoHoOV",
		"@SaeedMohammadiha"
	],
	"keywords": [
		"agribank",
		"htsc"
	],
	"scripts": {
		"build": "tsc --build && vite build",
		"dev-generate-clients": "vite build && node ./dist/cli.js generate-clients --url http://10.0.189.141:32102/swagger/v1/swagger.json --out ./src/generate-clients/.generated/ --replace-endpoint-regex /api/,/customer-management/ --replace-endpoint-regex /customer-management\\/CustomerManager/,/CustomerManager/ --customize-operation-id method_last-segment:pascalcase --skip-spec-validations true",
		"dev-create-template": "vite build && node ./dist/cli.js create --out ./src/create-template/.generated/",
		"lint": "eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"import": "./dist/cli.js",
			"types": "./dist/cli.d.ts"
		}
	},
	"files": [
		"dist/",
		"README.md",
		"package.json",
		"CHANGELOG.md"
	],
	"bin": {
		"agribank": "./dist/cli.js"
	},
	"dependencies": {
		"@apidevtools/swagger-parser": "^10.1.0",
		"@openapitools/openapi-generator-cli": "^2.15.0",
		"commander": "^12.1.0",
		"openapi3-ts": "^4.4.0",
		"ts-to-zod": "^3.13.0",
		"typed-openapi": "^0.9.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^22.8.2",
		"@typescript-eslint/eslint-plugin": "^8.8.1",
		"@typescript-eslint/parser": "^8.1.1",
		"typescript": "~5.6.2",
		"eslint": "^9.12.0",
		"eslint-plugin-n": "^17.11.1",
		"typescript-eslint": "^8.8.1",
		"vite": "^5.4.9",
		"vite-plugin-dts": "^4.3.0",
		"vite-tsconfig-paths": "^5.0.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
