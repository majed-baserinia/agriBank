{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"rootDir": ".",
		"allowJs": true,
		"checkJs": true,
		"declaration": true,
		"emitDeclarationOnly": true,
		"outDir": "./dist/types",
		"declarationMap": true,
		"noEmit": false
	},
	"include": ["**/*.js", "**/*.mjs"],
	"exclude": ["node_modules", "dist", "./eslint.config.mjs"]
}
