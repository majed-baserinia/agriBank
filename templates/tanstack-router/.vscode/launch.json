{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"command": "pnpm run dev",
			"name": "pnpm:dev",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "chrome:dev",
			"url": "http://localhost:5173/__APP_NAME__/",
			"webRoot": "${workspaceFolder}"
		},
		{
			"command": "pnpm exec baran-playground",
			"name": "pnpm:playground",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"command": "pnpm exec playwright codegen",
			"name": "pnpm:playwright:codegen",
			"request": "launch",
			"type": "node-terminal"
		}
	]
}
